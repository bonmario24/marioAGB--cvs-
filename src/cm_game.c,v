/*
                                  
	’ÊM’fâƒGƒ‰[‰æ–Ê
                                  
*/
const u8 ErrChr[3] = {
    0, 1, 2  //, 2, 1,
};
void multi_error(void)
{
    OamData	*oam;
    u8		i;

//    ObjVanishAll();
//    mGm_OAMIDX = 0;
    for(i=0; i<3; i++){
	oam = &OamBak[mGm_OAMIDX++];
	oam->VPos = 0x48;
	oam->Shape = ST_OAM_V_RECTANGLE;
	oam->HPos = 0x6C + i * 8;
	oam->Size = 0;
	oam->CharNo = ErrChr[i];
	oam->Pltt = 0x0E;
    }
}

/*
 ****************************************************************
 *     ƒƒCƒ“ƒQ[ƒ€ƒƒjƒ…[”z—ñ
